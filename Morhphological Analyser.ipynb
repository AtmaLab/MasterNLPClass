   "source": [
    "import nltk\n",
    "stemmer = nltk.PorterStemmer()\n",
    "verbs = ['appears', 'appear', 'appeared', 'calling','called']\n",
    "stems = []\n",
    "for verb in verbs:\n",
    "    stemmed_verb = stemmer.stem(verb)\n",
    "    stems.append(stemmed_verb)\n",
    "sorted(set(stems))"
   ]
